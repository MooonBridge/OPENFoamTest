FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
	T
    {
        solver          PCG; // Preconditioned Conjugate Gradient solver
        preconditioner  DIC; // Diagonal Incomplete Cholesky preconditioner
        tolerance       1e-06; // Convergence tolerance
        relTol          0; // Relative tolerance
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 2;
}


